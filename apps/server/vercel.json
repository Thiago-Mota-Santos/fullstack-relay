{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
    "rewrites": [
      {
        "source": "/",
        "missing": [
          {
            "type": "cookie",
            "key": "graphic-token"
          }
        ],
        "destination": "/auth/signin"
      }
    ]
}

